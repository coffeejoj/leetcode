struct TreeNode {
    int val;
    TreeNode *left;
    TreeNode *right;
    TreeNode(int x) : val(x), left(NULL), right(NULL) {}
};

void func(TreeNode *root, vector<int> &res)
{
    if(!root) return;
    func(root->left, res);
    res.push_back(root->val);
    func(root->right, res);
}

class Solution {
public:
    vector<int> inorderTraversal(TreeNode *root) {
        vector<int> res;
        func(root, res);
        return res;
    }
};